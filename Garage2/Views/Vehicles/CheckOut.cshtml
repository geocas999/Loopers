@model Garage2.Models.Vehicle

@{
    ViewBag.Title = "Kör ut";
}

<h2>Kör ut</h2>

<h3>Är du säker på att du vill köra ut ditt fordon?</h3>
<div>
    <h4>Fordon</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.VehicleType)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.VehicleType)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.RegNr)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RegNr)
        </dd>

        <dt>
