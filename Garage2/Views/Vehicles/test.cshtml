@model Garage2.Models.Vehicle

@{
    ViewBag.Title = "SearchId";
}

<h2>Sök Fordon via Registreringsnummer</h2>

<div class="col-md-6">

    <form method="get"
          action="@Url.Action("SearchRegNr")"
          data-tomas-ajax="true"
          data-tomas-target="#searchIdForCheckOut">

        <input id="inputRegister1"
               class="form-control"
               type="search"
               name="searchTerm"
               placeholder="Sök registreringsnummer"
               data-tomas-autocomplete="@Url.Action("Autocomplete")" />
    </form>
</div>

<div id="searchIdForCheckOut">

</div>